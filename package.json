{
  "name": "webapp",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "dev:local": "vite",
    "dev:sandbox": "wrangler pages dev dist --ip 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "wrangler pages dev dist",
    "preview:local": "wrangler pages dev dist --port 8788",
    "start": "npm run build && npm run preview:local",
    "deploy": "npm run build && wrangler pages deploy",
    "deploy:prod": "npm run build && wrangler pages deploy --project-name webapp",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "clean": "rm -rf dist node_modules/.vite",
    "clean-all": "rm -rf dist node_modules package-lock.json",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true",
    "test": "curl -s http://localhost:5173 || curl -s http://localhost:8788 || curl -s http://localhost:3000",
    "test:dev": "curl -s http://localhost:5173",
    "test:preview": "curl -s http://localhost:8788",
    "setup": "npm install && npm run build",
    "quick-start": "./QUICK_COMMANDS.sh",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop webapp",
    "pm2:restart": "pm2 restart webapp",
    "pm2:delete": "pm2 delete webapp",
    "pm2:logs": "pm2 logs webapp --nostream",
    "pm2:monitor": "pm2 monit",
    "git:commit": "git add . && git commit -m",
    "git:status": "git status",
    "git:push": "git push origin main"
  },
  "dependencies": {
    "hono": "^4.9.8"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "vite": "^6.3.5",
    "wrangler": "^4.42.0"
  }
}